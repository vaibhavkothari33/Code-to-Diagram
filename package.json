{
  "name": "code-to-diagram",
  "displayName": "Code to Diagram",
  "description": "Convert code to visual diagrams",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Visualization"
  ],
  "activationEvents": [
    "onCommand:extension.generateDiagram"
  ],
  "main": "./src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.generateDiagram",
        "title": "Generate Code Diagram"
      }
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "@types/vscode": "^1.60.0",
    "@types/node": "^14.0.0"
  }
}
